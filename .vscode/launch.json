{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C++: Debug active file (MinGW)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}\\${fileBasenameNoExtension}.exe",
      "args": [],
